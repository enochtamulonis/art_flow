  <div class="w-full h-full bg-blue-300 flex flex-col items-center pt-10">
    <div class="large-card-size flex flex-col items-center bg-gray-700">
      <h3 class="text-3xl my-10">All Listings</h3>
      <div class="w-full h-full flex justify-around flex-wrap">
        <% @paintings.each do |painting| %>
          <%= render 'admin_painting_tile', :painting => painting %>
        <% end %>
      </div>
    </div>
  </div>
  <div class="h-10 py-10 w-full bg-blue-300"> </div>
</div>
